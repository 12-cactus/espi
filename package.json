    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@types/jest": "^27.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
